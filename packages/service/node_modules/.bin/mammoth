#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/00-work/00-ai-mind/FastGPT-4.9.8/node_modules/.pnpm/mammoth@1.9.0/node_modules/mammoth/bin/node_modules:/mnt/d/00-work/00-ai-mind/FastGPT-4.9.8/node_modules/.pnpm/mammoth@1.9.0/node_modules/mammoth/node_modules:/mnt/d/00-work/00-ai-mind/FastGPT-4.9.8/node_modules/.pnpm/mammoth@1.9.0/node_modules:/mnt/d/00-work/00-ai-mind/FastGPT-4.9.8/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/00-work/00-ai-mind/FastGPT-4.9.8/node_modules/.pnpm/mammoth@1.9.0/node_modules/mammoth/bin/node_modules:/mnt/d/00-work/00-ai-mind/FastGPT-4.9.8/node_modules/.pnpm/mammoth@1.9.0/node_modules/mammoth/node_modules:/mnt/d/00-work/00-ai-mind/FastGPT-4.9.8/node_modules/.pnpm/mammoth@1.9.0/node_modules:/mnt/d/00-work/00-ai-mind/FastGPT-4.9.8/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mammoth/bin/mammoth" "$@"
else
  exec node  "$basedir/../mammoth/bin/mammoth" "$@"
fi
