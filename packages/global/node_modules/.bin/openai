#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/00-work/00-ai-mind/FastGPT-4.9.8/node_modules/.pnpm/openai@4.61.0_encoding@0.1.13_zod@3.24.2/node_modules/openai/bin/node_modules:/mnt/d/00-work/00-ai-mind/FastGPT-4.9.8/node_modules/.pnpm/openai@4.61.0_encoding@0.1.13_zod@3.24.2/node_modules/openai/node_modules:/mnt/d/00-work/00-ai-mind/FastGPT-4.9.8/node_modules/.pnpm/openai@4.61.0_encoding@0.1.13_zod@3.24.2/node_modules:/mnt/d/00-work/00-ai-mind/FastGPT-4.9.8/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/00-work/00-ai-mind/FastGPT-4.9.8/node_modules/.pnpm/openai@4.61.0_encoding@0.1.13_zod@3.24.2/node_modules/openai/bin/node_modules:/mnt/d/00-work/00-ai-mind/FastGPT-4.9.8/node_modules/.pnpm/openai@4.61.0_encoding@0.1.13_zod@3.24.2/node_modules/openai/node_modules:/mnt/d/00-work/00-ai-mind/FastGPT-4.9.8/node_modules/.pnpm/openai@4.61.0_encoding@0.1.13_zod@3.24.2/node_modules:/mnt/d/00-work/00-ai-mind/FastGPT-4.9.8/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../openai/bin/cli" "$@"
else
  exec node  "$basedir/../openai/bin/cli" "$@"
fi
